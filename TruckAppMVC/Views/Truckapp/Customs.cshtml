    .optCards {
        width: 100%;
        display: flex;
        align-items: center;
        justify-content: center;
        flex-wrap: wrap;
    }

        .optCards .stage1,
        .stage2,
        .stage3,
        .stage4,
        .other {
            width: 500px;
            height: 220px;
            display: flex;
            align-items: center;
            justify-content: center;
            margin: 20px;
            position: relative;
            border-radius: 10px;
            transition: all .5s cubic-bezier(0.68, -0.55, 0.265, 1.55);
            box-shadow: 0px 0px 50px #030e122a, inset 0px 0px 10px #030e121f;
            background-color: #fff;
            border: 5px solid #fff;
        }

            .optCards .stage1::before,
            .stage2::before,
            .stage3::before,
            .stage4::before,
            .other::before {
                content: '';
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                background-color: #170422;
                clip-path: circle(15% at left 0%);
                border-radius: 10px;
                transition: all .5s cubic-bezier(0.68, -0.55, 0.265, 1.55);
            }

            .optCards .stage1::before,
            .stage3::before {
                background: linear-gradient(#f6013e, #f63a01);
            }



            .optCards .stage1::after,
            .stage2::after,
            .stage3::after,
            .stage4::after,
            .other::after {
                content: '';
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                background: linear-gradient(#01bdf6, #012ef6);
                border-radius: 10px;
                clip-path: circle(15% at right 100%);
                transition: all .5s cubic-bezier(0.68, -0.55, 0.265, 1.55);
            }

            .optCards .stage1::after,
            .stage3::after {
                background: #170422;
            }


            .optCards .stage1:hover::after,
            .stage2:hover::after,
            .stage3:hover::after,
            .stage4:hover::after,
            .other:hover::after {
                clip-path: circle(100% at right 100%);
                transition: all .5s cubic-bezier(0.68, -0.55, 0.265, 1.55);
            }

            .optCards .stage1:hover::before,
            .stage2:hover::before,
            .stage3:hover::before,
            .stage4:hover::before,
            .other:hover::before {
                clip-path: circle(100% at left 0%);
                transition: all .5s cubic-bezier(0.68, -0.55, 0.265, 1.55);
            }

            .optCards .stage1:hover .stagebtn,
            .stage2:hover .stagebtn,
            .stage3:hover .stagebtn,
            .stage4:hover .stagebtn,
            .other:hover .stagebtn {
                color: #fff;
            }

            .optCards .stage1:hover,
            .stage2:hover,
            .stage3:hover,
            .stage4:hover,
            .other:hover {
                transform: scale(1.05);
