<script>


    $(document).ready(function () {


        $(' .alert > .fa-times').click(function () {

            $('.alertlayer').removeClass('unlockalertlayer')
            $('.alert').removeClass('unlockalert')
            $('.alert').addClass('lockalert')
            $('.alertlayer').addClass('lockalertlayer')
            $('.nav,.cont').removeClass('blur')


        })

        $(' .cbxalert > .fa-times').click(function () {

            $('.cbxalertlayer').removeClass('unlockcbxalertlayer')
            $('.cbxalert').removeClass('unlockcbxalert')
            $('.cbxalert').addClass('lockcbxalert')
            $('.cbxalertlayer').addClass('lockcbxalertlayer')
            $('.nav,.cont').removeClass('blur')

        })

        $(' .cbxsuccess > .fa-times').click(function () {

            $('.cbxsuccesslayer').removeClass('unlockcbxsuccesslayer')
            $('.cbxsuccess').removeClass('unlockcbxsuccess')
            $('.cbxsuccess').addClass('lockcbxsuccess')
            $('.cbxsuccesslayer').addClass('lockcbxsuccesslayer')
            $('.nav,.cont').removeClass('blur')

        })



        $(' .netalert > .fa-times').click(function () {

            $('.netalertlayer').removeClass('unlocknetalertlayer')
            $('.netalert').removeClass('unlocknetalert')
            $('.netalert').addClass('locknetalert')
            $('.netalertlayer').addClass('locknetalertlayer')
            $('.nav,.cont').removeClass('blur')
            window.location.reload(true);
        })



        $('.other').click(function () {

            $('.otherlayer').addClass('unlocklayer')
            $('.otherform').addClass('unlockoform')
            $('.otherform').removeClass('lockform')
            $('.otherlayer').removeClass('locklayer')
            $('.nav,.cont').addClass('blur')


        })

        $('#ofs').click(function () {

            var otherpicname = $('#otherinpfield').val().trim();

            if (otherpicname.length <= 0) {
                $('.otherform>p').show()

                $('.otherform > p').html("Please enter the name of the photo.")
                return false;
            }
            else {
                return true;
            }

        })


        $('#ofc').click(function () {

            $('.otherlayer').addClass('locklayer')
            $('.otherform').addClass('lockform')
            $('.otherform').removeClass('unlockoform')
            $('.otherlayer').removeClass('unlocklayer')
            $('.nav,.cont').removeClass('blur')

            $('.otherform>p').hide()

            return false;
        })

        $('#submitotherform').click(function () {


            if ($('input[type=checkbox]:checked').length <= 0) {

                $('.nav,.cont').addClass('blur')

                $('.cbxalertlayer').addClass('unlockcbxalertlayer')
                $('.cbxalert').addClass('unlockcbxalert')
                $('.cbxalert').removeClass('lockcbxalert')
                $('.cbxalertlayer').removeClass('lockcbxalertlayer')

                $('.cbxalert').removeClass('successbgc')
                $('.cbxalert>.error').addClass('fa-exclamation')
                $('.cbxalert>.error').removeClass('fa-check')
                $('.cbxalert>.errormsg').html('<h3>Please check "I have taken all the photos needed for this stage"</h3>')

                return false;
            }

            else {
                $('.cbxalertlayer').removeClass('unlockcbxalertlayer')
                $('.cbxalert').removeClass('unlockcbxalert')

                return true;


            }

        })



        // checking online or offline

        $('a,button').click(function () {

            var network = navigator.onLine;

            if (network !== true) {

                $('.netalert').addClass('unlocknetalert')
                $('.netalertlayer').addClass('unlocknetalertlayer')
                $('.netalert>.errormsg').html('<h2>Please check your internet connection !</h2>')
                $('.alertlayer,.cbxalertlayer,.cbxsuccesslayer,.otherlayer').addClass('nodisplay')
                $('.netalert').removeClass('locknetalert')
                $('.netalertlayer').removeClass('locknetalertlayer')

                $('.nav,.cont').addClass('blur')

                return false;

            }
            else {
                $('.alertlayer,.cbxalertlayer,.cbxsuccesslayer,.otherlayer').removeClass('nodisplay')

                return true;
            }
        })

    })
</script>

</html>