        .cbx label {
            margin-left: 40px;
            font-weight: 300;
            color: #030e12;
            font-size: 16px;
            position: relative;
            cursor: pointer;
        }

        .cbx input[type="checkbox"] {
            appearance: none;
        }

        .cbx .labelforcbx::before {
            content: '';
            position: absolute;
            top: 0%;
            left: -10%;
            width: 20px;
            height: 20px;
            background-color: #fff;
            border-radius: 3px;
            border: 2px solid #adadad;
            transition: all .3s ease-in-out;
        }

        .cbx input[type="checkbox"]:checked ~ .labelforcbx::before {
            content: '✔';
            color: #fff;
            background-color: #01f60d;
            border-radius: 3px;
            border: 2px solid #01f60d;
            box-shadow: 0px 0px 10px #01f60d;
